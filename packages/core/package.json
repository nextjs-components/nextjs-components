{
  "name": "nextjs-components",
  "version": "0.1.0-rc52",
  "author": {
    "name": "Kevin Wang",
    "url": "https://thekevinwang.com"
  },
  "description": "React components transcribed from the Vercel design system.",
  "source": "dist/index.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "emit-types": "npx tsc -p tsconfig.build.json",
    "build:swc": "npx swc src --copy-files -d dist && npm run emit-types",
    "build:babel": "babel src --extensions '.ts','.tsx' --copy-files -d dist --ignore '**/*.test.*' && npm run emit-types",
    "clean": "rm -rf dist/",
    "prepublish": "npm run clean && npm run build:babel",
    "publish:local": "npx yalc publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextjs-components/nextjs-components.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nextjs-components/nextjs-components/issues"
  },
  "homepage": "https://github.com/nextjs-components/nextjs-components#readme",
  "dependencies": {
    "@react-aria/button": "3.3.4",
    "@react-aria/dialog": "3.1.4",
    "@react-aria/focus": "3.5.0",
    "@react-aria/interactions": "^3.7.0",
    "@react-aria/overlays": "3.7.2",
    "@react-aria/radio": "^3.1.6",
    "@react-aria/ssr": "^3.1.0",
    "@react-stately/overlays": "3.1.3",
    "@react-stately/radio": "^3.3.2",
    "clsx": "^1.1.1",
    "react-is": "^17.0.2",
    "react-live": "^2.3.0",
    "react-use-measure": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.124",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-is": "^17.0.3",
    "jest": "^27.4.7",
    "jest-css-modules-transform": "^4.3.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "next": ">=10.x",
    "react": ">=16.8",
    "styled-jsx": ">=4.x"
  },
  "overrides": {
    "react-simple-code-editor": {
      "react": "17.0.2",
      "react-dom": "17.0.2"
    }
  }
}
